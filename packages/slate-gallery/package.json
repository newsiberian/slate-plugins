    "prebuild": "yarn clean",
    "prepublishOnly": "yarn clean && yarn build"
  },
  "dependencies": {
    "@mercuriya/slate-gallery-common": "0.8.0-alpha.3",
    "@mercuriya/slate-gallery-read-only": "0.8.0-alpha.3",
    "tslib": "^2.5.2"
  },
  "peerDependencies": {
    "array-move": "^3.0.1",
    "react": "*",
    "react-dropzone": "^14.2.3",
